#include <iostream.h>
#include <conio.h>
using namespace std;

int main()
{
    char A[9999];
    int a,b,c;
    cout<<"vvedite chsla: ";
    fgets(A,9999,stdin);
    a=strlen(A)-2;
    for(int k=0; k<a;k++)
    {
     for(int i=0; i<a;i++)
     {
      c=i+1;
      if(A[i]>A[c]; 
      {
       b=A[i];
       A[i]=A[c];
       A[c]=b; 
      }
     }
    }
     
     for(int j=0; j<=a;j++)
     {
      if(j!=0)
      {
       c=j-1;
       if(A[j]!=A[c])
       {
        cout<<A[j];
        }
       }
       else
       {
       cout<<A[j];
      }
     }
getch();
return 0;
}
