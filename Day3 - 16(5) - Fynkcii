#include<iostream.h>
#include<conio.h>
#include<math.h>

float formula(){
      float y, a=-10, b=10, h=0.5;
      for(float x=a; x<=b; x+=h){
                y=(cos(x)*cos(x))/(exp(x)-1);
                cout<<"x="<<x<<"\t\ty="<<y<<endl;}
      }
      
int main(){
   
    formula();
   
    getch();
    return 0;
}
